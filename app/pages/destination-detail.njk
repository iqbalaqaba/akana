                                <span class="c-card-date mt-2 mb-2">
                                    <i class="far fa-calendar-alt pr-2"></i>
                                    3 Days 2 Night
                                </span>
                                <span class="c-card-dest">
                                    <i class="fas fa-map-marker-alt pr-2"></i>
                                    Bali
                                </span>
                            </p>
                            <a href="#" class="btn btn-primary btn-block btn--primary">
                                packages detail
                            </a>
                        </div>
                    </div>
                </div>
                <!-- card product #1 end here -->

