                            <!-- title start here -->
                            <h3 class="text-center">
                                explore this destination packages
                            </h3>
                            <!-- title end here -->

                            <!-- searchbox start here -->
                            <div class="card c-card mx-auto">
                                <div class="card-body">
                                    <form class="mb-0 form-inline">
                                        <input type="text" class="form-control form-control-lg" placeholder="Cari disini...">
                                        <button type="submit" class="btn btn--primary ml-3">search</button>
                                    </form>
                                </div>
                            </div>
                            <!-- searchbox end here -->

                        </div>
                    </div>
                </div>
                <!-- search end here -->

                <!-- package destination package start here -->

                <!-- card product #1 start here -->
                <div class="col-lg-4 mt-5">
